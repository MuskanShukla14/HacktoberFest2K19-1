#include <stdio.h>
int main(){
	int num;
	scanf("%d", &num);              
    while(num--)
    {
        
        int i,n;
        long long s=0;
        scanf("%d",&n);
        for(i=1; i<n; i++)
       
        {
           if(n%i==0)
            s+=i;
       
        }
      
        printf("%lld\n",s);
   
    }
}


